<Grid x:Class="MetoCraft.Home"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MetoCraft"
      mc:Ignorable="d" 
      d:DesignHeight="719" d:DesignWidth="792">


    <Grid>
        <WrapPanel ItemHeight="200" ItemWidth="200" HorizontalAlignment="Center">
            <Button x:Name="button" Content="Play" BorderThickness="2" Background="White" BorderBrush="White" Style="{DynamicResource ButtonMe}" FontSize="13.333" Click="button_Click"/>
            <Button x:Name="button1" Content="Settings" Style="{DynamicResource ButtonMe}" BorderThickness="2" BorderBrush="White" Background="White"/>
        </WrapPanel>

    </Grid>
</Grid>
