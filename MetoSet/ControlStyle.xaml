                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="False">
                            <Setter Property="Grid.Column" TargetName="border2" Value="0"/>
                            <Setter Property="Background" TargetName="border2" Value="{DynamicResource Button.Static.Border}"/>
                            <Setter Property="BorderBrush" TargetName="border1" Value="{DynamicResource Button.Static.Border}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Grid.Column" TargetName="border2" Value="2"/>
                            <Setter Property="Background" TargetName="border1" Value="{DynamicResource Button.Checked.Border}"/>
                            <Setter Property="Background" TargetName="border2" Value="{DynamicResource Button.Checked.Background}"/>
                            <Setter Property="BorderBrush" TargetName="border1" Value="{DynamicResource Button.Checked.Border}"/>
                        </Trigger>
                        <Trigger Property="Button.IsDefaulted" Value="true"/>
                        <Trigger Property="IsMouseOver" Value="True"/>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsPressed" Value="True"/>
                                <Condition Property="IsChecked" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Background" TargetName="border1" Value="{DynamicResource Button.Pressed.Border}"/>
                            <Setter Property="BorderBrush" TargetName="border1" Value="{DynamicResource Button.Pressed.Border}"/>
                            <Setter Property="Background" TargetName="border2" Value="{DynamicResource Button.Pressed.Background}"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsPressed" Value="True"/>
                                <Condition Property="IsChecked" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="BorderBrush" TargetName="border1" Value="{DynamicResource Button.Pressed.Border}"/>
                            <Setter Property="Background" TargetName="border1" Value="{DynamicResource Button.Pressed.Border}"/>
                            <Setter Property="Background" TargetName="border2" Value="{DynamicResource Button.Pressed.Background}"/>
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="false"/>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsChecked" Value="False"/>
                                <Condition Property="IsMouseOver" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Background" TargetName="border2" Value="{DynamicResource Button.MouseOver.Border}"/>
                            <Setter Property="BorderBrush" TargetName="border1" Value="{DynamicResource Button.MouseOver.Border}"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsChecked" Value="True"/>
                                <Condition Property="IsMouseOver" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Background" TargetName="border2" Value="{DynamicResource Button.Checked.Background}"/>
                            <Setter Property="Background" TargetName="border1" Value="{DynamicResource Button.Checked.MouseOver.Border}"/>
                            <Setter Property="BorderBrush" TargetName="border1" Value="{DynamicResource Button.Checked.MouseOver.Border}"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
